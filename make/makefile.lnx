# Legacy makefile for Linux systems
# No Husky support

BINDIR = /usr/local/bin
CFGDIR = /usr/local/etc
CC     = gcc
CFLAGS = -Wall -O3 -DCFGDIR=\"$(CFGDIR)\"

OBJ=.o
EXE=
TPU=.tpu
RM=rm
LIB=.a
AR=ar
AR_R=rs


%.o: ../%.c
	$(CC) -c $(CFLAGS) ../$*.c 

include makefile.inc

all: commonall

install:
	install -s emailpkt $(BINDIR)

clean: commonclean

distclean: commondistclean
